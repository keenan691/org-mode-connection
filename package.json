{
  "name": "org-mode-connection",
  "version": "0.8.8",
  "description": "Read, write, synchronize and query emacs's org-mode files.",
  "main": "dist/index.js",
  "types": "src/index.d.ts",
  "keywords": [
    "emacs",
    "org mode",
    "parser",
    "database",
    "file",
    "synchronization"
  ],
  "engineStrict": true,
  "engines": {
    "node": "~10"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "^7.1.5",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "@types/ramda": "^0.25.41",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "jest": "^29.3.1",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.1",
    "promisify-node": "^0.5.0",
    "realm": "^2.19.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.17.2",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "check-types": "tsc",
    "build-declarations": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel ./src --out-dir dist",
    "serve": "nodemon -w src --exec babel ./src --out-dir dist --extensions '.ts,.tsx,.js'",
    "start": "nodemon --exec babel-node --extensions '.ts,.tsx,.js' src/index.js",
    "doc": "nodemon --exec typedoc --out ~/Chaos/documentation/ src --ignoreCompilerErrors --includeDeclarations --excludeExternals"
  },
  "author": "Bart≈Çomiej Nankiewicz (https://bartlomiej.nankiewicz@gmail.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bnankiewicz/org-mode-connection.git"
  },
  "bugs": {
    "url": "https://github.com/bnankiewicz/org-mode-connection/issues"
  },
  "homepage": "https://github.com/bnankiewicz/org-mode-connection#readme",
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "global": "^4.3.2",
    "moment": "^2.22.2",
    "ramda": "^0.25.0"
  }
}
